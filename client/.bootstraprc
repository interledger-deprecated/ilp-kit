bootstrapVersion: 3

styleLoaders:
  - style-loader
  - css-loader
  - sass-loader?includePaths[]=src/theme&includePaths[]=node_modules

appStyles: ./src/theme/bootstrap.overrides.scss

styles:
  mixins: true
  normalize: true
  print: true
  glyphicons: true
  scaffolding: true
  type: true
  code: true
  grid: true
  tables: true
  forms: true
  buttons: true
  component-animations: true
  dropdowns: true
  button-groups: true
  input-groups: true
  navs: true
  navbar: true
  breadcrumbs: true
  pagination: true
  pager: true
  labels: true
  badges: true
  jumbotron: true
  thumbnails: true
  alerts: true
  progress-bars: true
  media: true
  list-group: true
  panels: true
  wells: true
  responsive-embed: true
  close: true
  modals: true
  tooltip: true
  popovers: true
  carousel: true
  utilities: true
  responsive-utilities: true
